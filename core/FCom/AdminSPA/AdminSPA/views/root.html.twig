<!DOCTYPE html>
<html>
<head>
    <script>
        var SvAppData = ({{ data|json_encode|raw }});
    </script>
    {{ THIS.view('head') | raw }}
</head>
<body>
{% verbatim %}
<div id="sv-app" @click="pageClick()" v-show="isLoaded">
    <sv-comp-header v-if="isLoggedIn"></sv-comp-header>
    <main :class="{'main-nav-opened':$store.state.ui.windowWidth>=1024, 'main-nav-closed':$store.state.ui.windowWidth<1024}">
        <div class="f-admin-container">
            <sv-comp-nav v-if="isLoggedIn"></sv-comp-nav>
			
            <div class="f-admin-main-view ">
				<sv-comp-header-breadcrumbs :mobile="true"></sv-comp-header-breadcrumbs>
				<router-view></router-view>
            </div>
        </div>
    </main>
    <sv-comp-messages></sv-comp-messages>
</div>
{% endverbatim %}
</body>
</html>